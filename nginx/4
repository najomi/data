link: http://brainspl.at/nginx.conf.txt
desc: Сжимать данные перед отдачей
code: |
  gzip            on;
  gzip_http_version 1.0;
  gzip_comp_level 2;
  gzip_proxied any;
  gzip_types 
        text/plain text/html text/css 
        application/x-javascript text/xml 
        application/xml application/xml+rss 
        text/javascript;
